{
    "name":"xkcd Enhancer",
    "version":"0.1.3",
    "description":"Adds some minor enhancements to xkcd.com.",
    "author":"Spencer Harston",
    "icons":{
        "16":"img/icon.png",
        "32":"img/icon.png",
        "48":"img/icon.png",
        "128":"img/icon.png"
    },
    "content_scripts":[
        {
            "run_at":"document_end",
            "matches":["https://xkcd.com/*"],
            "js":["script.js"],
            "css":["style.css"]
        }
    ],
    "manifest_version":2
}